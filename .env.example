# required (lowercase)
aws_access_key_id=
aws_secret_access_key=
aws_session_token=

# region + s3 (accepts lowercase or uppercase; shown lowercase)
aws_region=us-east-1
s3_bucket=cons71-validator-dev
s3_prefix=evaluation/data/chunks/

# tuning (accepts lowercase or uppercase)
chunk_bytes=6710000      # for JSONL: ~MB per uncompressed chunk
items_per_chunk=200000     # for ARRAY: items per chunk
max_concurrency=8
compress=gzip              # default compression if CLI flag not provided